@{
}

@model List<Cadeteria.Entidades.ClaseCadetes>

<table border="1">
    <tr>
        <th>ID</th>
        <th>Nombre</th>
        <th>DNI</th>
        <th>Domicilio</th>
        <th>Telefono</th>
    </tr>
    @foreach (var item in Model)
    {
<tr>
    <td>@item.Id_cadete</td>
    <td>@item.Apellido_cadete @item.Nombre_cadete</td>
    <td>@item.Dni_cadete</td>
    <td>@item.Domicilio_cadete</td>
    <td>@item.Telefono_cadete</td>
</tr>
    }

</table>
