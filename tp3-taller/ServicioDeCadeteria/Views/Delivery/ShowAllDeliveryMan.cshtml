@{
}

@model List<ServicioDeCadeteria.Entities.DeliveryClass>

<table>
    <tr>
        <th>id</th>
        <th>Name</th>
        <th>Adress</th>
        <th>Telephone number</th>
    </tr>
    <tr>
        @foreach (var item in Model)
        {
            <td>@item.DeliveryMan_id</td>
            <td>@item.DeliveryMan_name</td>
            <td>@item.DeliveryMan_adress</td>
            <td>@item.DeliveryMan_telephone</td>
        }
    </tr>
</table>